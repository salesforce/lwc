{
    "extends": "eslint:recommended",
    "parser": "typescript-eslint-parser",
    "env": {
        "es6": true,
        "browser": true,
        "jest": true
    },
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module"
    },
    "plugins": [
        "typescript"
    ],
    "rules": {
        "no-console": 0
    },
    "globals": {
        "process": true,
        "benchmark": true,
        "before": true,
        "run": true,
        "after": true
    }
}

{
  "name": "lwc-integration",
  "private": true,
  "version": "0.19.0-0",
  "license": "Beerware",
  "scripts": {
    "test": "echo 'No unit test in lwc-integration package'",
    "build": "node scripts/build.js",
    "build:dev": "MODE=dev yarn build",
    "build:prod": "MODE=prod yarn build",
    "build:compat": "MODE=compat yarn build",
    "build:prod_compat": "MODE=prod_compat yarn build",
    "start": "http-server -c-1 -p 8080",
    "local": "yarn local:prod",
    "local:dev": "yarn build:dev && MODE=dev wdio ./scripts/wdio.conf.js",
    "local:prod": "yarn build:prod && MODE=prod wdio ./scripts/wdio.conf.js",
    "local:compat": "yarn build:compat && MODE=compat wdio ./scripts/wdio.conf.js",
    "local:prod_compat": "yarn build:prod_compat && MODE=prod_compat wdio ./scripts/wdio.conf.js",
    "sauce": "yarn sauce:prod",
    "sauce:dev": "MODE=dev yarn build:dev && MODE=dev wdio ./scripts/wdio.sauce.conf.js",
    "sauce:prod": "MODE=prod yarn build:prod && MODE=prod wdio ./scripts/wdio.sauce.conf.js",
    "sauce:compat": "MODE=compat yarn build:compat && MODE=compat wdio ./scripts/wdio.sauce.conf.js",
    "sauce:prod_compat": "MODE=prod_compat yarn build:prod_compat && MODE=prod_compat wdio ./scripts/wdio.sauce.conf.js"
  },
  "devDependencies": {
    "babel-preset-compat": "0.17.35",
    "compat-polyfills": "0.17.35",
    "deepmerge": "^1.5.2",
    "dotenv": "^4.0.0",
    "fs-extra": "^4.0.2",
    "http-server": "^0.10.0",
    "lwc-compiler": "0.19.0-0",
    "lwc-engine": "0.19.0-0",
    "minimist": "^1.2.0",
    "rollup-plugin-compat": "0.17.35",
    "rollup-plugin-lwc-compiler": "0.19.0-0",
    "wdio-junit-reporter": "^0.3.1",
    "wdio-mocha-framework": "^0.5.10",
    "wdio-sauce-service": "^0.4.0",
    "wdio-selenium-standalone-service": "~0.0.10",
    "wdio-spec-reporter": "^0.1.0",
    "wdio-static-server-service": "^1.0.1",
    "webdriverio": "^4.8.0"
  }
}

{
  "name": "lwc-engine",
  "version": "0.17.14",
  "description": "Raptor Engine",
  "main": "dist/commonjs/es2017/engine.js",
  "module": "dist/modules/es2017/engine.js",
  "typings": "types/engine.d.ts",
  "scripts": {
    "clean": "rimraf dist/",
    "test": "jest --coverage --maxWorkers=2",
    "types": "tsc --noEmit",
    "lint": "tslint -p .",
    "build": "concurrently \"npm run build:es-and-cjs\" \"npm run build:umd:prod\" \"npm run build:umd:dev\"",
    "build:umd:dev": "rollup -c scripts/rollup.config.umd.dev.js",
    "build:umd:prod": "rollup -c scripts/rollup.config.umd.prod.js",
    "build:es-and-cjs": "rollup -c scripts/rollup.config.es-and-cjs.js"
  },
  "devDependencies": {
    "concurrently": "^3.5.1",
    "jest": "^21.2.1",
    "rimraf": "^2.6.1",
    "rollup": "~0.50.0",
    "rollup-plugin-inject": "^1.4.1",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-typescript": "^0.8.1",
    "rollup-plugin-uglify": "3.0.0",
    "ts-jest": "^21.0.1",
    "tslint": "^5.2.0",
    "typescript": "^2.6.2",
    "uglify-es": "3.3.4"
  },
  "lwc": {
    "modules": {
      "engine": "dist/modules/es2017/engine.js"
    }
  }
}

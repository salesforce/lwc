{
    "extends": "eslint:recommended",
    "parser": "typescript-eslint-parser",
    "env": {
        "es6": true,
        "jest": true
    },
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module"
    },
    "plugins": [
        "typescript"
    ],
    "rules": {
        "no-console": "off"
    }
}

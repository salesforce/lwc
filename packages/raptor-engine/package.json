{
  "name": "raptor-engine",
  "version": "0.15.2",
  "description": "Raptor Engine",
  "main": "dist/commonjs/es2017/engine.js",
  "module": "dist/modules/es2017/engine.js",
  "scripts": {
    "clean": "rimraf dist/ lib/",
    "test": "jest --coverage",
    "types": "tsc src/**/*.ts --noEmit",
    "lint": "eslint src/**/*.ts",
    "build": "npm run build:es-and-cjs && npm run build:umd:prod && npm run build:umd:dev",
    "build:umd:dev": "rollup -c scripts/rollup.config.umd.dev.js",
    "build:umd:prod": "rollup -c scripts/rollup.config.umd.prod.js",
    "build:es-and-cjs": "rollup -c scripts/rollup.config.es-and-cjs.js",
    "prepublish": "yarn run build"
  },
  "publishConfig": {
    "registry": "https://nexus.soma.salesforce.com/nexus/content/repositories/npmjs-internal/"
  },
  "devDependencies": {
    "eslint": "^3.19.0",
    "eslint-plugin-typescript": "^0.1.0",
    "jest": "^21.2.1",
    "rimraf": "^2.6.1",
    "rollup": "~0.50.0",
    "rollup-plugin-inject": "^1.4.1",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-strip-caridy-patched": "^1.1.1",
    "rollup-plugin-typescript": "^0.8.1",
    "rollup-plugin-uglify": "^2.0.1",
    "snabbdom": "^0.5.4",
    "ts-jest": "^21.0.1",
    "typescript": "^2.3.4",
    "typescript-eslint-parser": "^3.0.0",
    "uglify-es": "^3.1.3"
  }
}

{
    "name": "@lwc/engine-core",
    "version": "2.35.0",
    "description": "Core LWC engine APIs.",
    "homepage": "https://lwc.dev/",
    "repository": {
        "type": "git",
        "url": "https://github.com/salesforce/lwc.git",
        "directory": "packages/@lwc/engine-core"
    },
    "bugs": {
        "url": "https://github.com/salesforce/lwc/issues"
    },
    "main": "dist/engine-core.cjs.js",
    "module": "dist/engine-core.js",
    "typings": "types/index.d.ts",
    "license": "MIT",
    "scripts": {
        "build": "rollup --config scripts/rollup.config.js",
        "dev": "rollup  --config scripts/rollup.config.js --watch --no-watch.clearScreen"
    },
    "files": [
        "dist/",
        "types/"
    ],
    "dependencies": {
        "@lwc/aria-reflection": "2.35.0",
        "@lwc/features": "2.35.0",
        "@lwc/shared": "2.35.0"
    },
    "devDependencies": {
        "observable-membrane": "2.0.0"
    },
    "publishConfig": {
        "access": "public"
    },
    "nx": {
        "targets": {
            "build": {
                "outputs": [
                    "./dist",
                    "./types"
                ]
            }
        }
    }
}

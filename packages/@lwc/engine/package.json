{
  "name": "@lwc/engine",
  "version": "0.35.1",
  "description": "LWC Engine",
  "main": "dist/commonjs/es2017/engine.js",
  "module": "dist/modules/es2017/engine.js",
  "typings": "types/engine.d.ts",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf dist/ lib/",
    "lint": "tslint -p ./tsconfig.json",
